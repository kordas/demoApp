<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns		= "http://www.w3.org/ns/widgets"
	xmlns:gap	= "http://phonegap.com/ns/1.0"
	id		= "com.kordas.demoApp"
	version 	= "1.0.0">

	<name>Testing Css</name>

	<description>First Try Demo App</description>

	<gap:platforms>
		<gap:platform name="android" minVersion="2.1" />
		<gap:platform name="webos" />
		<gap:platform name="symbian.wrt" />
		<gap:platform name="blackberry" project="widgets"/>
	</gap:platforms>
	
	<icon src="blurry.png" gap:role="default" />

    <gap:splash src="weirdo.png" />
	
	<preference name="fullscreen" value="true" />
	<preference name="splash-screen-duration" value="10000" />
	<preference name="disable-cursor" value="true" />
	<preference name="permissions" value="none"/>

</widget>
